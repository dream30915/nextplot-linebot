{
    "version": 2,
    "buildCommand": "echo 'Build complete'",
    "outputDirectory": ".",
    "framework": null,
    "regions": [
        "sin1"
    ],
    "env": {
        "APP_NAME": "NextPlot",
        "APP_ENV": "production",
        "APP_DEBUG": "false"
    },
    "rewrites": [
        {
            "source": "/api/line/webhook",
            "destination": "/api/webhook"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "maxDuration": 10
        }
    }
}