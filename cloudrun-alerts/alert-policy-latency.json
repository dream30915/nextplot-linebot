{
  "displayName": "Cloud Run latency p95 high",
  "combiner": "OR",
  "enabled": true,
  "conditions": [
    {
      "displayName": "p95 latency > 1000ms for 5m",
      "conditionThreshold": {
        "filter": "metric.type=\"run.googleapis.com/request_latencies\" resource.type=\"cloud_run_revision\" resource.label.service_name=\"nextplot-linebot\"",
        "comparison": "COMPARISON_GT",
        "thresholdValue": 1000,
        "duration": "300s",
        "aggregations": [
          { "alignmentPeriod": "60s", "perSeriesAligner": "ALIGN_PERCENTILE_95" }
        ]
      }
    }
  ],
  "notificationChannels": []
}
